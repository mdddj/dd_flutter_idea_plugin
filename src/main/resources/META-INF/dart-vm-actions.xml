<idea-plugin>
    <actions>
        <action id="FlutterVmRefreshTreeAction" class="shop.itbug.fluttercheckversionx.actions.vm.widget.RefreshTreeAction"  />
        <action id="FlutterVmDetailedModeToggleAction" class="shop.itbug.fluttercheckversionx.actions.vm.widget.DetailedModeToggleAction" />
        <action id="FlutterVmInspectorOverlayToggleAction" class="shop.itbug.fluttercheckversionx.actions.vm.widget.InspectorOverlayToggleAction" />
        <group id="FlutterVMTreeToolbarAction">
            <reference ref="FlutterVmRefreshTreeAction"/>
            <reference ref="FlutterVmDetailedModeToggleAction"/>
