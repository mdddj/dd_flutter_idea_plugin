        <action class="shop.itbug.flutterx.actions.bar.OpenAndroidProjectOnASAction"
                id="OpenAndroidProjectOnASAction"/>
        <action class="shop.itbug.flutterx.actions.bar.OpenIosProjectInXcodeAction"
