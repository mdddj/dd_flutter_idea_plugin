        <action id="DioRequestBodyToFreezedAction"
                class="shop.itbug.flutterx.actions.DioRequestBodyToFreezedAction" text="Json to Freezed"
                icon="/icons/freezed.svg"/>
        <group id="FlutterX Window Top Actions">
            <reference ref="Dio.Request.Item.Render.Option"/>
            <reference ref="shop.itbug.flutterx.form.socket.DioRequestCleanAction"/>
            <reference ref="shop.itbug.flutterx.actions.OpenSettingAnAction"/>
            <reference ref="FlutterProjects"/>
        </group>


        <!--        左侧操作菜单-->
        <group id="FlutterX window Left Action">
            <reference ref="Dio.Request.Item.Render.Option"/>
            <separator/>
            <reference ref="shop.itbug.flutterx.form.socket.DioRequestCleanAction"/>
            <reference ref="shop.itbug.flutterx.actions.OpenSettingAnAction"/>
        </group>


        <!--        获取 hive盒子列表操作-->
        <action class="shop.itbug.flutterx.hive.action.HiveGetBoxListAction" id="GetHiveBoxList"
                icon="AllIcons.Actions.Refresh"/>
        <!--        hive 的使用说明-->
        <action id="HiveTutorialHelp" class="shop.itbug.flutterx.hive.action.HiveTutorialHelp"
                icon="AllIcons.Actions.Help" description="Hive help" text="Help"/>

        <!--        hive 的操作工具栏-->
        <group id="shop.itbug.flutterx.hive.action.HiveDefaultActionGroup">
            <reference ref="FlutterProjects"/>
            <reference ref="GetHiveBoxList"/>
            <!--            <reference id="HiveTutorialHelp"/>-->
            <reference ref="HelpAction"/>
        </group>


        <!--  riverpod 操作 -->
        <action id="StatelessToConsumer" class="shop.itbug.flutterx.actions.riverpod.StatelessToConsumer"
                text="StatelessWidget to ConsumerWidget">
            <keyboard-shortcut keymap="$default" first-keystroke="meta alt M"/>
        </action>
        <action id="StatefulToConsumer"
                class="shop.itbug.flutterx.actions.riverpod.StatefulToConsumerAction"
                text="StatefulWidget to ConsumerStatefulWidget"/>
        <action id="RiverpodActionsSetting"
