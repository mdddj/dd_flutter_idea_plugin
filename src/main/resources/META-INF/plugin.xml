        <postStartupActivity implementation="shop.itbug.flutterx.services.MyAssetGenPostStart"/>
        <postStartupActivity implementation="shop.itbug.flutterx.services.PubspecStartActivity"/>
        <postStartupActivity implementation="shop.itbug.flutterx.hints.FlutterAssetsStartHandle"/>
        <postStartupActivity implementation="shop.itbug.flutterx.services.UserDartLibServiceInit"/>
        <!--        错误处理-->
        <!--                <errorHandler implementation="shop.itbug.flutterx.common.PluginErrorHandle"/>-->
        <!--        <configurationType implementation="shop.itbug.flutterx.run.FlutterXRunConfigType"/>-->
