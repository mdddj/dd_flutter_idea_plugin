                language="yaml"/>
        <!--        <codeInsight.daemonBoundCodeVisionProvider-->
        <!-- implementation="shop.itbug.flutterx.inlay.dartfile.DartUseageCountInlay"/>-->

        <externalAnnotator implementationClass="shop.itbug.flutterx.tools.DartPluginVersionCheckV2"
                           language="yaml"/>

        <codeInsight.declarativeInlayProvider group="TYPES_GROUP"
                                              implementationClass="shop.itbug.flutterx.inlay.dartfile.DartTypeInlayProvider"
                                              isEnabledByDefault="true" language="Dart" bundle="messages.pluginBundle"
                                              nameKey="dart_type_params_show_name_key" providerId="DartTypeShow"
                                              descriptionKey="dart_type_params_show_name_key"/>
        <codeInsight.declarativeInlayProviderCustomSettingsProvider
                implementationClass="shop.itbug.flutterx.inlay.dartfile.DartTypeInlaySettingProvider"
                language="Dart" providerId="DartTypeShow"/>

        <codeInsight.inlayActionHandler handlerId="dartTypeInlayProviderId"
                                        implementationClass="shop.itbug.flutterx.inlay.dartfile.DartTypeClickActionHandle"/>

        <!--        <codeInsight.inlayProvider-->
        <!-- implementationClass="shop.itbug.flutterx.inlay.dartfile.DartStringIconShowInlay"-->
        <!-- language="Dart"/>-->


        <!--   Json编辑器悬停显示预览图片     -->
        <lang.documentationProvider implementationClass="shop.itbug.flutterx.inlay.json.JsonPreviewDocument"
                                    language="JSON"/>

        <!--    pubspec.yaml插鼠标悬停显示插件信息     -->
        <lang.documentationProvider implementationClass="shop.itbug.flutterx.document.YamlDocument"
                                    language="yaml" order="first"/>

        <!--    dart文档的HTML实现    -->
        <lang.documentationProvider implementationClass="shop.itbug.flutterx.document.DartDocumentExt"
                                    language="Dart"
                                    order="first, after Translation.TranslatedDocumentationProvider.Dart"/>

        <!--   dio监听http请求小窗口   -->
        <toolWindow factoryClass="shop.itbug.flutterx.window.FlutterXSocketWindow" id="Dio Request"
                    anchor="bottom" icon="/icons/flutter.svg"/>
        <toolWindow factoryClass="shop.itbug.flutterx.window.DartVmServiceWindowsFactory"
                    id="FlutterX Dart VM" icon="/icons/dart.svg" anchor="bottom" doNotActivateOnStart="true"/>

        <!--   底部状态栏展示小部件     -->
        <statusBarWidgetFactory implementation="shop.itbug.flutterx.services.MyUserBarFactory"
                                id="myUserBarFactory"/>

        <!--        <diff.DiffExtension implementation="shop.itbug.flutterx.diff.PubYamlGithubAndPathDiff"/>-->

        <!--        系统设置-->
        <projectConfigurable instance="shop.itbug.flutterx.setting.AppConfig" parentId="language"
                             id="setting.flutterx" displayName="FlutterX"/>

        <projectConfigurable instance="shop.itbug.flutterx.setting.vm.DartVmSetting"
                             parentId="setting.flutterx" id="dart.vm.test" displayName="Dart Vm"/>

        <editorNotificationProvider
                implementation="shop.itbug.flutterx.notif.PubPluginVersionCheckNotification"/>
        <editorNotificationProvider
                implementation="shop.itbug.flutterx.notif.DartFreezedClassFixNotification"/>


        <!--        插件新版本检测-->

