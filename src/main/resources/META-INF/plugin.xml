        <action id="GoToDiscordAction" class="shop.itbug.flutterx.actions.bar.GoToDiscordAction"/>
        <action id="GoToQQGroupAction" class="shop.itbug.flutterx.actions.bar.GoToQQGroupAction"/>
        <action id="GoToDocumentAction" class="shop.itbug.flutterx.actions.bar.GoToDocumentAction"/>
        <!--        <action id="shop.itbug.flutterx.dialog.macro.DartMacroAction"-->
