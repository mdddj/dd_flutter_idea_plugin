    <extensions defaultExtensionNs="org.jetbrains.kotlin">
        <supportsKotlinPluginMode supportsK2="true"/>
    </extensions>
    <extensions defaultExtensionNs="com.intellij">
        <intentionAction>
            <className>shop.itbug.flutterx.actions.internal.L10nKeyExtractIntentionAction</className>
            <language>Dart</language>
        </intentionAction>
        <intentionAction>
            <className>shop.itbug.flutterx.actions.internal.DartAddFinalPropsIntentionAction</className>
            <language>Dart</language>
        </intentionAction>
        <intentionAction>
            <className>shop.itbug.flutterx.actions.internal.DartAddSimpleConstructionIntentionAction
            </className>
            <language>Dart</language>
        </intentionAction>

        <annotator language="Dart" implementationClass="shop.itbug.flutterx.annotator.DartAssetsAnnotator"/>

        <annotator language="yaml"
                   implementationClass="shop.itbug.flutterx.annotator.PubspecPackageDiscontinuedAnnotator"/>

        <notificationGroup id="plugin_new_version_notify" displayType="BALLOON"/>
        <notificationGroup displayType="BALLOON" id="dio_socket_notify"/>
        <notificationGroup displayType="BALLOON" id="flutter_version_check"/>
        <notificationGroup displayType="BALLOON" id="json_to_freezed_tooltip"/>
        <notificationGroup displayType="BALLOON" id="dart_package_check_service"/>
        <notificationGroup displayType="BALLOON" id="flutterx_check_unused_package"/>
        <notificationGroup displayType="BALLOON" id="open_flutter_project_in_ide"/>
        <notificationGroup displayType="BALLOON" id="DartVmServiceListener"/>

        <!--    排水沟图标    -->
        <codeInsight.lineMarkerProvider language="yaml"
                                        implementationClass="shop.itbug.flutterx.linemark.PluginDartIconLineMark"/>

        <!--    plugin版本自动联想功能    -->

        <completion.contributor language="yaml" order="first"
                                implementationClass="shop.itbug.flutterx.autoCompletion.DartPackageAutoCompletion"/>
        <completion.contributor language="Dart"
                                implementationClass="shop.itbug.flutterx.hints.AssetsFilePathAutoComplete"/>
        <completion.contributor language="Dart"
                                implementationClass="shop.itbug.flutterx.autoCompletion.DartPartAutoCompletion"/>
        <completion.contributor language="Dart"
                                implementationClass="shop.itbug.flutterx.hints.FreezedPartAutoComplicate"/>
        <completion.contributor language="Dart"
                                implementationClass="shop.itbug.flutterx.hints.IpHostAutoCompletionContributor"/>

        <codeInsight.declarativeInlayProvider group="OTHER_GROUP"
                                              implementationClass="shop.itbug.flutterx.inlay.yaml.PubspecPackageUpdateTimeShowInlay"
                                              isEnabledByDefault="true" language="yaml" nameKey="lastupdate_date_time"
                                              bundle="messages.pluginBundle"
                                              providerId="flutterxWithPubspecUpdateTime"/>


        <codeInsight.daemonBoundCodeVisionProvider
                implementation="shop.itbug.flutterx.inlay.dartfile.DartWidgetToRiverpodWidgetCodeVisit"/>
        <codeInsight.daemonBoundCodeVisionProvider
                implementation="shop.itbug.flutterx.inlay.dartfile.DartFileInsertPartOfString"/>
        <codeInsight.daemonBoundCodeVisionProvider
                implementation="shop.itbug.flutterx.inlay.dartfile.DartAssetsIconInlineShow"/>
        <config.codeVisionGroupSettingProvider
                implementation="shop.itbug.flutterx.inlay.dartfile.DartAssetsIconInlineShowSettingGroup"
                id="DartAssetsIconInlineShowSettingGroup"/>
        <codeInsight.daemonBoundCodeVisionProvider
                implementation="shop.itbug.flutterx.inlay.freezed.FreezedActionInlay"/>
        <config.codeVisionGroupSettingProvider
                implementation="shop.itbug.flutterx.inlay.freezed.FreezedActionInlaySettingProvider"
                id="FreezedActionInlaySettingProvider"/>
        <codeInsight.daemonBoundCodeVisionProvider
                implementation="shop.itbug.flutterx.inlay.gradle.GradleAliyuMirrorImageInlay"/>
        <config.codeVisionGroupSettingProvider
                implementation="shop.itbug.flutterx.inlay.gradle.GradleAliyuMirrorImageInlayGroupSetting"
                id="GradleAliyuMirrorImageInlayGroupSetting"/>
        <codeInsight.daemonBoundCodeVisionProvider
                implementation="shop.itbug.flutterx.inlay.gradle.KtsAliyunMirrorInlay"/>
        <config.codeVisionGroupSettingProvider
                implementation="shop.itbug.flutterx.inlay.gradle.KtsAliyunMirrorInlayGroupSetting"
                id="KtsAliyunMirrorInlayGroupSetting"/>
        <!--        <codeInsight.daemonBoundCodeVisionProvider-->
        <!-- implementation="shop.itbug.flutterx.inlay.yaml.DartPackageSearchDialogCodeVision"/>-->
        <!--        <config.codeVisionGroupSettingProvider-->
        <!-- implementation="shop.itbug.flutterx.inlay.yaml.DartPackageSearchDialogCodeVisionGroupSetting"-->
        <!-- id="DartPackageSearchDialogCodeVisionGroupSetting"/>-->

        <codeInsight.inlayProvider
                implementationClass="shop.itbug.flutterx.inlay.yaml.DartPackageSearchDialogInlay"
