                  topic="com.intellij.execution.ExecutionListener"/>
        <listener class="shop.itbug.flutterx.services.PubspecServiceFileListener"
                  topic="com.intellij.openapi.vfs.newvfs.BulkFileListener"/>
    </projectListeners>

    <actions>


        <action id="OpenInVsCodeAction" class="shop.itbug.flutterx.actions.yaml.OpenInVsCodeAction"
